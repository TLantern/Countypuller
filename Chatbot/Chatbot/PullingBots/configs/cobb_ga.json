{
  "name": "Cobb County GA Lien Index",
  "state": "GA",
  "base_url": "https://search.gsccca.org",
  "description": "Cobb County Georgia lien records via GSCCCA system",
  "scraper_type": "search_form",
  "field_mappings": [
    {
      "field_name": "case_number",
      "field_type": "case_number",
      "selectors": [
        {
          "selector": "td:nth-child(1)",
          "selector_type": "css",
          "attribute": "text",
          "required": true,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "validation": "^[A-Z0-9-]+$",
      "requires_ocr": false
    },
    {
      "field_name": "document_type",
      "field_type": "text",
      "selectors": [
        {
          "selector": "td:nth-child(2)",
          "selector_type": "css",
          "attribute": "text",
          "required": true,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "requires_ocr": false
    },
    {
      "field_name": "filing_date",
      "field_type": "date",
      "selectors": [
        {
          "selector": "td:nth-child(3)",
          "selector_type": "css",
          "attribute": "text",
          "required": true,
          "multiple": false
        }
      ],
      "post_process": "parse_date",
      "requires_ocr": false
    },
    {
      "field_name": "debtor_name",
      "field_type": "person_name",
      "selectors": [
        {
          "selector": "td:nth-child(4)",
          "selector_type": "css",
          "attribute": "text",
          "required": false,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "requires_ocr": false
    },
    {
      "field_name": "claimant_name",
      "field_type": "person_name",
      "selectors": [
        {
          "selector": "td:nth-child(5)",
          "selector_type": "css",
          "attribute": "text",
          "required": false,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "requires_ocr": false
    },
    {
      "field_name": "county",
      "field_type": "text",
      "selectors": [
        {
          "selector": "td:nth-child(6)",
          "selector_type": "css",
          "attribute": "text",
          "required": true,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "requires_ocr": false
    },
    {
      "field_name": "book_page",
      "field_type": "text",
      "selectors": [
        {
          "selector": "td:nth-child(7)",
          "selector_type": "css",
          "attribute": "text",
          "required": false,
          "multiple": false
        }
      ],
      "post_process": "clean_whitespace",
      "requires_ocr": false
    },
    {
      "field_name": "document_link",
      "field_type": "url",
      "selectors": [
        {
          "selector": "td a",
          "selector_type": "css",
          "attribute": "href",
          "required": false,
          "multiple": false
        }
      ],
      "requires_ocr": false
    }
  ],
  "search_config": {
    "search_url": "https://search.gsccca.org/Lien/namesearch.asp",
    "search_form_selector": "form",
    "search_fields": {
      "party_type": "select[name='PartyType']",
      "instrument_type": "select[name='InstrumentType']",
      "county": "select[name='County']",
      "search_name": "input[name='SearchName']",
      "date_from": "input[name='DateFrom']",
      "date_to": "input[name='DateTo']",
      "results_per_page": "select[name='DisplayResults']"
    },
    "submit_button_selector": "input[type='submit']",
    "results_container_selector": "table"
  },
  "pagination_config": {
    "pagination_type": "numbered",
    "next_button_selector": "a[href*='Page=']",
    "page_number_selector": "a[href*='Page=']",
    "results_per_page": 50,
    "max_pages": 20
  },
  "auth_config": null,
  "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/125.0.0.0 Safari/537.36",
  "headless": true,
  "timeout": 30,
  "delay_between_requests": 2.0,
  "duplicate_check_field": "case_number",
  "required_fields": ["case_number", "document_type", "filing_date", "county"],
  "javascript_enabled": true,
  "cookies_file": null,
  "proxy_config": null,
  "custom_headers": {
    "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
    "Accept-Language": "en-US,en;q=0.5",
    "Accept-Encoding": "gzip, deflate, br",
    "Connection": "keep-alive",
    "Upgrade-Insecure-Requests": "1"
  },
  "created_at": "2024-12-28T10:30:00",
  "updated_at": "2024-12-28T10:30:00",
  "version": "1.0.0",
  "tested": false
} 